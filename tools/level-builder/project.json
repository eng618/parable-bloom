{
  "name": "level-builder",
  "projectType": "library",
  "sourceRoot": "tools/level-builder",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task lb:build",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
