{
  "name": "level-builder",
  "projectType": "library",
  "sourceRoot": "tools/level-builder",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/level-builder"
      ],
      "options": {
        "command": "task lb:build",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task lb:test",
        "cwd": "."
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task lb:lint",
        "cwd": "."
      }
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task levels:validate-solvable",
        "cwd": "."
      }
    },
    "tutorials-validate": {
      "executor": "nx:run-commands",
      "cache": true,
      "options": {
        "command": "task levels:tutorials:validate-solvable",
        "cwd": "."
      }
    }
  }
}
