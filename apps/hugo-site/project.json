{
  "name": "hugo-site",
  "projectType": "application",
  "sourceRoot": "apps/hugo-site",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/public"
      ],
      "options": {
        "command": "task hugo:build",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task hugo:serve",
        "cwd": "."
      }
    }
  }
}
