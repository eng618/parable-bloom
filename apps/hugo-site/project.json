{
  "name": "hugo-site",
  "projectType": "application",
  "sourceRoot": "apps/hugo-site",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task hugo:build",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task hugo:serve",
        "cwd": "."
      }
    }
  },
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "apps/hugo-site"
      }
    }
  }
}
