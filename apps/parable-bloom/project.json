{
  "name": "parable-bloom",
  "projectType": "application",
  "sourceRoot": "apps/parable-bloom",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/build/web"
      ],
      "options": {
        "command": "task flutter:build:web",
        "cwd": "."
      },
      "configurations": {
        "preview": {
          "command": "task flutter:build:web:preview"
        },
        "prod": {
          "command": "task flutter:build:web:prod"
        }
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:test",
        "cwd": "."
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:lint",
        "cwd": "."
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task firebase:deploy:all",
        "cwd": "."
      }
    },
    "release-web": {
      "executor": "nx:run-commands",
      "cache": true,
      "outputs": [
        "{projectRoot}/build/web"
      ],
      "options": {
        "command": "task flutter:build:web",
        "cwd": "."
      }
    },
    "release-android": {
      "executor": "nx:run-commands",
      "cache": true,
      "outputs": [
        "{projectRoot}/build/app/outputs/bundle/release"
      ],
      "options": {
        "command": "task flutter:build:appbundle",
        "cwd": "."
      }
    },
    "deploy-web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task firebase:hosting:deploy",
        "cwd": "."
      }
    },
    "deploy-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/parable-bloom/android && fastlane deploy",
        "cwd": "."
      }
    }
  }
}
