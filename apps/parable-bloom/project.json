{
  "name": "parable-bloom",
  "projectType": "application",
  "sourceRoot": "apps/parable-bloom",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:build:web",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:test",
        "cwd": "."
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:lint",
        "cwd": "."
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task firebase:deploy:all",
        "cwd": "."
      }
    },
    "release-web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:build:web",
        "cwd": "."
      }
    },
    "release-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task flutter:build:appbundle",
        "cwd": "."
      }
    },
    "deploy-web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "task firebase:hosting:deploy",
        "cwd": "."
      }
    },
    "deploy-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/parable-bloom/android && fastlane deploy",
        "cwd": "."
      }
    }
  },
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "apps/parable-bloom",
        "currentVersionResolver": "version-file",
        "versionFile": "apps/parable-bloom/pubspec.yaml"
      }
    }
  }
}
